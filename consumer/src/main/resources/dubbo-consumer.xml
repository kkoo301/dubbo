<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <dubbo:application name="demo-consumer"/>

    <dubbo:annotation package="com.asiainfo"/>

    <dubbo:registry protocol="zookeeper" address="127.0.0.1:2181" client="curator" register="false" group="test" />

    <!--<dubbo:protocol name="dubbo" port="20880" serialization="java" accesslog="true" />-->

    <dubbo:consumer timeout="1000"/>

</beans>